{% extends 'layout.html.twig' %}

{% block content %}
    <h1>Edition catégorie</h1>
    {{ form_start(form) }} {# ouvre la balise form #}
    {{ form_row(form.name) }}{# élement de formulaire pour name #}
    <div class="form-btn-group pull-right">
        <button type="submit" class="btn btn-primary">Enregistrer</button>
        <a href="{{path('app_admin_category_list')}}" class="btn btn-default">Retour</a>
    </div>
    {{ form_end(form)}} {# ferme la balise form #}
{% endblock %}